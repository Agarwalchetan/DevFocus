MONGO_URL=mongodb://localhost:27017
DB_NAME=devfocus
CORS_ORIGINS=http://localhost:3000

# AI Configuration for Insights
USE_AI_INSIGHTS=true

# Provider API keys
OPENAI_API_KEY=N/A
ANTHROPIC_API_KEY=N/A
GOOGLE_API_KEY=N/A
GROQ_API_KEY=your_api_key_here

# Model routing (recommended - Groq is primary for speed and cost-effectiveness)
AI_MODELS='[
    {
        "provider":"groq",
        "model":"openai/gpt-oss-120b",
        "apiKeyEnv":"GROQ_API_KEY",
        "isUsed":true
    },
    {
        "provider":"openai",
        "model":"gpt-4o-mini",
        "apiKeyEnv":"OPENAI_API_KEY",
        "isUsed":false
    },
    {
        "provider":"anthropic",
        "model":"claude-3-5-sonnet-latest",
        "apiKeyEnv":"ANTHROPIC_API_KEY",
        "isUsed":false
    },
    {
        "provider":"gemini",
        "model":"gemini-1.5-pro",
        "apiKeyEnv":"GOOGLE_API_KEY",
        "isUsed":false
    }
]'


# Optional legacy fallback (only used if AI_MODELS is not set)
AI_PRIMARY_PROVIDER=groq
AI_PRIMARY_MODEL=openai/gpt-oss-120b
AI_SECONDARY_PROVIDER=openai
AI_SECONDARY_MODEL=gpt-4o-mini
AI_TERTIARY_PROVIDER=gemini
AI_TERTIARY_MODEL=gemini-1.5-pro
